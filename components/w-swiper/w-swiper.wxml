<swiper class="swiper" circular indicator-dots autoplay interval="2000" indicator-color="#fff" indicator-active-color='#ff5777'>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item" item-id="">
      <image src="{{item.image}}" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>
